{% extends 'base.html.twig' %}

{% block title %}
    Create Ticket - {{ parent() }}
{% endblock %}

{% block body %}
    <h1>Ticket creation</h1>

    {{ form_start(form) }}
    {{ form_widget(form) }}
    Message : <input type="text" name="message">
    <input type="submit" value="Create" class="btn btn-primary">
    {{ form_end(form) }}

    <hr>

    <a href="{{ path('ticket_index') }}" class="btn btn-primary">Back to the list</a>
{% endblock %}
